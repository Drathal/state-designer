{
  "name": "State Designer",
  "version": 2,
  "alias": ["statedesigner.com", "state-designer"],
  "builds": [
    {
      "src": "docs",
      "use": "@now/static-build",
      "config": {
        "distDir": "public"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "docs/$1",
      "continue": true
    }
  ]
}
